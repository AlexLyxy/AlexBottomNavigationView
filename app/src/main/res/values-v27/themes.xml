<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.Theme.AlexBottomNavigationView" parent="Theme.AppCompat.DayNight">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/material_deep_teal_200</item>
        <item name="colorPrimaryDark">@color/material_deep_teal_200</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowTitleSize">100dp</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="actionBarSize">100dp</item>
        <item name="android:statusBarColor">@color/material_deep_teal_200</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="statusBarBackground">@color/material_deep_teal_200</item>
        <item name="statusBarScrim">@color/black</item>
        <item name="android:navigationBarColor">@color/material_deep_teal_200</item>
        <item name="android:navigationBarDividerColor">@color/white</item>
        <item name="android:navigationIcon">@color/error_color_material_dark</item>
        <item name="android:customNavigationLayout">@style/ThemeOverlay.AppCompat.Light</item>

        <item name="actionBarItemBackground">@color/black</item>
        <item name="panelBackground">@color/black</item>


    </style>
</resources>